<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="co.grandcircus.RideHard.utils.SearchLocationAbreviations, co.grandcircus.RideHard.utils"
		table="country" entity-name="Country">
		<id name="code" access="field" column="code">
			<generator class="assigned" />
		</id>
		<property name="name" column="name" type="string" />
	</class>
	<class
		name="co.grandcircus.RideHard.utils.SearchLocationAbreviations, co.grandcircus.RideHard.utils"
		table="state" entity-name="State">
		<id name="code" access="field" column="code">
			<generator class="assigned" />
		</id>
		<property name="name" column="name" type="string" />
	</class>

</hibernate-mapping>